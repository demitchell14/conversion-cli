{
  "name": "conversion-cli",
  "description": "Node CLI Conversion tool used to convert database data",
  "version": "0.0.0",
  "authors": [
    "Devin Mitchell @demitchell14",
    "Bill Wilder @ids-wilder"
  ],
  "author": "Integrated Data Services @2019",
  "bin": {
    "cli": "./bin/run"
  },
  "bugs": "https://github.com/demitchell14/conversion-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2.1.6",
    "dotenv": "^6.1.0",
    "msnodesqlv8": "^0.6.12",
    "mssql": "^5.0.4",
    "nodemon": "^1.18.10",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/dotenv": "^6.1.0",
    "@types/mssql": "^4.0.13",
    "@types/node": "^10.14.4",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.11.0",
    "typescript": "^3.3.4000"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/demitchell14/conversion-cli",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "demitchell14/conversion-cli",
  "scripts": {
    "start": "node ./bin/run convert idmp names -l 250",
    "alias": "node ./bin/run convert idap alias -l 250",
    "watch": "tsc -w",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
